{
  "name": "cosmos-fetch",
  "version": "0.0.10",
  "description": "A fetch wrapper for cosmos blockchains",
  "repository": "angelorc/cosmos-fetch",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./drivers/*": {
      "types": "./dist/drivers/*.d.ts",
      "import": "./dist/drivers/*.mjs",
      "require": "./dist/drivers/*.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "play": "jiti playground",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "generate": "json2ts ./src/chain.schema.json > ./src/chain.schema.ts"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@vercel/kv": "^3.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "automd": "^0.3.8",
    "changelogen": "^0.5.5",
    "eslint": "^9.8.0",
    "eslint-config-unjs": "^0.3.2",
    "idb-keyval": "^6.2.1",
    "ioredis": "^5.4.1",
    "jiti": "^2.0.0-beta.3",
    "json-schema-to-typescript": "^15.0.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "unbuild": "^3.0.0-rc.7",
    "vitest": "^2.0.5"
  },
  "packageManager": "pnpm@9.7.0",
  "dependencies": {
    "ofetch": "^1.4.1",
    "unstorage": "^1.12.0"
  },
  "peerDependencies": {
    "idb-keyval": "^6.2.1",
    "ioredis": "^5.4.1",
    "@vercel/kv": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "idb-keyval": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "@vercel/kv": {
      "optional": true
    }
  }
}
